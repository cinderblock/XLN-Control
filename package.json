{
  "name": "XLN-Control",
  "version": "0.0.1",
  "description": "Libraries and a GUI for interfacing with XLN series programmable DC power supplies",
  "main": "main.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --compilers js:babel/register --recursive",
    "test-watch": "npm test -- --watch",
    "lint": "eslint .",
    "hot-server": "node server.js",
    "build": "NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "start": "electron .",
    "start-hot": "HOT=1 NODE_ENV=development electron .",
    "package": "NODE_ENV=production node package.js",
    "package-all": "npm run package -- --all"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cinderblock/jsXLN.git"
  },
  "keywords": [
    "XLN",
    "XLN6024",
    "SCPI"
  ],
  "author": "GitHub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cinderblock/jsXLN/issues"
  },
  "homepage": "https://github.com/cinderblock/jsXLN#readme",
  "devDependencies": {
    "asar": "^0.8.2",
    "babel-core": "^6.1.2",
    "babel-eslint": "^4.1.4",
    "babel-loader": "^6.0.1",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-0": "^6.1.2",
    "bootstrap": "^3.3.5",
    "chai": "^3.4.0",
    "css-loader": "^0.22.0",
    "del": "^2.0.2",
    "electron-packager": "^5.1.1",
    "electron-prebuilt": "^0.34.3",
    "electron-rebuild": "^1.0.2",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "0.1.0",
    "eslint-plugin-react": "^3.7.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "font-awesome": "^4.4.0",
    "history": "^1.13.0",
    "minimist": "^1.2.0",
    "mocha": "^2.3.3",
    "node-libs-browser": "^0.5.3",
    "proxyquire": "^1.7.3",
    "react-bootstrap": "^0.27.3",
    "react-dom": "^0.14.2",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.3",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1",
    "webpack-target-electron-renderer": "^0.1.1",
    "xln": "0.6.1"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "electron-debug": "^0.3.2",
    "flux": "^2.1.1",
    "keymirror": "^0.1.1",
    "object-assign": "^4.0.1",
    "react": "^0.14.2",
    "react-router": "^1.0.0-rc4",
    "react-smoothie": "0.0.4"
  }
}
